{
  "name": "generator-micro-service",
  "version": "1.0.0",
  "description": "Create a Node.js micro-service skeleton",
  "homepage": "https://github.com/telstra-cip/generator-micro-service",
  "author": "Versent",
  "files": [
    "index.js",
    "generators"
  ],
  "main": "index.js",
  "keywords": [
    "yeoman-generator"
  ],
  "devDependencies": {
    "@types/jest": "^22.2.2",
    "@types/node": "^9.6.2",
    "aws-sdk-mock": "2.0.0",
    "del": "^3.0.0",
    "env-cmd": "^8.0.1",
    "gulp": "^3.9.1",
    "gulp-add-src": "^1.0.0",
    "gulp-tslint": "^8.1.3",
    "gulp-typescript": "^4.0.2",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "jest-cli": "^21.2.1",
    "merge2": "^1.2.1",
    "nsp": "^2.8.0",
    "prettier": "^1.11.1",
    "supertest": "^3.1.0",
    "ts-jest": "^22.4.2",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.11.0",
    "typescript": "^2.8.1",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^1.7.0"
  },
  "repository": "https://github.com/telstra-cip/generator-micro-service",
  "scripts": {
    "build": "npm install && gulp",
    "test": "env-cmd .env.test jest",
    "prepublishOnly": "nsp check"
  },
  "dependencies": {
    "aws-sdk": "^2.195.0",
    "express": "^4.16.2",
    "chalk": "^2.3.2",
    "generator-jest": "^1.5.1",
    "git-remote-origin-url": "^2.0.0",
    "inquirer-npm-name": "^3.0.0",
    "http": "0.0.0",
    "https": "^1.0.0",
    "lodash": "4.17.10",
    "parse-author": "^2.0.0",
    "request": "^2.85.0",
    "request-promise": "^4.2.2",
    "serverless-http": "^1.5.3",
    "validate-npm-package-name": "^3.0.0",
    "winston": "^3.0.0-rc5",
    "yeoman-generator": "^2.0.3"
  },
  "jest": {
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 75,
        "functions": 80,
        "lines": 80,
        "statements": -10
      }
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|(\\.|/)(spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "^@root/(.*)": "<rootDir>/src/$1",
      "^@common/(.*)": "<rootDir>/src/common/$1",
      "^@services/(.*)": "<rootDir>/src/services/$1"
    }
  },
  "license": "ISC",
  "engines": {
    "npm": ">= 4.0.0"
  }
}
